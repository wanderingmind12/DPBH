{
  "manifest_version": 2,
  "name": "Checkbox Auto-Check",
  "version": "1.0",
  "description": "Automatically checks checkboxes on web pages.",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
